    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "backstop": "^1.0.1",
    "eslint": "7.10.0",
    "eslint-config-prettier": "^7.2.0",
